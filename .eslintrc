{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base"],
    "rules": {
        "semi": "off",
        "no-console": "off",
        "no-plusplus": "warn",
        "eol-last": "off",
        "no-tabs": "off",
        "no-alert": "off",
        "prefer-destructuring": "off"
    }
}
